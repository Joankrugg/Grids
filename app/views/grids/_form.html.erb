<%= simple_form_for [@song, @grid ] do |f| %>
  <%= f.association :section %>
  <%= f.input :location, collection: 1...8 %>
  <%= f.association :type %>
  <%= f.input :grid_location, collection: 1...8 %>
  <%= f.association :musical_note %>
  <%= f.association :musical_scale %>
  <%= f.association :second_musical_note %>
  <%= f.association :second_musical_scale %>
  <%= f.association :third_musical_note %>
  <%= f.association :third_musical_scale %>
  <%= f.submit "Submit grid", class: "btn btn-primary" %>
<% end %>
