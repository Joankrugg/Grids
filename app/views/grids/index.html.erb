        <div class="row">
          <% intro.each do |grid| %>
            <%= link_to edit_song_grid_path(@song, grid) do %>
              <div class='grid'>
                <div class='grid-info'>
                  <% if grid.type.present? %>
                    <% case grid.type.id %>
                      <% when 1 %>
                        <%= cl_image_tag("Single_vkhxvm.png", height:120, width: 140, crop: :fit, class: "grid") %>
