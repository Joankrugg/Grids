<div class="container">
  <div class="grid-page">
    <%= link_to song_path(@song) do %>
      <h3 class="title"><%= @song.title.capitalize %></h3>
    <% end %>
    <% intro = [] %>
    <% verse = [] %>
    <% prechorus = [] %>
    <% chorus = [] %>
    <% bridge = [] %>
    <% casse = [] %>
    <% solo = [] %>
    <% outro = [] %>
    <% b = @song.grids.sort_by { |grid| grid[:grid_location] } %>
      <% b.each do |grid| %>
        <% if grid.section.id == 1 %>
          <% intro << grid %>
        <% end %>
        <% if grid.section.id == 2 %>
          <% verse << grid %>
        <% end %>
        <% if grid.section.id == 3 %>
          <% prechorus << grid %>
        <% end %>
        <% if grid.section.id == 4 %>
          <% chorus << grid %>
        <% end %>
        <% if grid.section.id == 5 %>
          <% bridge << grid %>
        <% end %>
        <% if grid.section.id == 6 %>
          <% casse << grid %>
        <% end %>
        <% if grid.section.id == 7 %>
          <% solo << grid %>
        <% end %>
        <% if grid.section.id == 8 %>
          <% outro << grid %>
        <% end %>
      <% end %>
      <% if intro.empty? == false %>
