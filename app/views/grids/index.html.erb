  <% second_section = [] %>
  <% third_section = [] %>
  <% fourth_section = [] %>
  <% fifth_section = [] %>
  <% sixth_section = [] %>
  <% seventh_section = [] %>
  <% eighth_section = [] %>
  <% b = @song.grids.sort_by { |grid| grid[:grid_location] } %>
