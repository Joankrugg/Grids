      <% if grid.location == 1 %>
        <% first_section << grid %>
      <% end %>
      <% if grid.location == 2 %>
        <% second_section << grid %>
      <% end %>
      <% if grid.location == 3 %>
        <% third_section << grid %>
      <% end %>
      <% if grid.location == 4 %>
        <% fourth_section << grid %>
      <% end %>
      <% if grid.location == 5 %>
        <% fifth_section << grid %>
      <% end %>
      <% if grid.location == 6 %>
        <% sixth_section << grid %>
      <% end %>
      <% if grid.location == 7 %>
        <% seventh_section << grid %>
      <% end %>
      <% if grid.location == 8 %>
        <% eighth_section << grid %>
      <% end %>
